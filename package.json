{
  "name": "get-unity",
  "description": "Command line tool for getting the download URL for the latest or specific version of Unity.",
  "version": "2.0.0",
  "main": "index.js",
  "type": "module",
  "bin": {
    "get-unity": "bin/index.js"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "5.0.1",
    "jsdom": "20.0.0",
    "meow": "10.1.3",
    "node-fetch": "3.2.6",
    "read-pkg-up": "9.1.0",
    "update-notifier": "6.0.2"
  },
  "devDependencies": {
    "jest": "28.1.2"
  },
  "scripts": {
    "start": "node bin/index.js",
    "test": "npx cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "update": "node bin/index.js --force",
    "postinstall": "npm run update"
  },
  "keywords": [
    "unity"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "homepage": "https://github.com/neogeek/get-unity",
  "repository": {
    "type": "git",
    "url": "git://github.com/neogeek/get-unity.git"
  }
}
